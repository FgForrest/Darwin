<?xml version="1.0" encoding="windows-1250"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd">

	<bean id="darwinStorageLockPersisterToTest" class="one.edee.darwin.storage.DefaultDatabaseLockStorage">
		<property name="dataSource" ref="dataSource"/>
		<property name="resourceAccessor" ref="darwinResourceAccessor4Test"/>
	</bean>

	<bean id="darwinResourceAccessor4Test" class="one.edee.darwin.resources.DefaultResourceAccessor">
		<property name="resourcePath" value="classpath:/META-INF/darwin/sql"/>
	</bean>

</beans>